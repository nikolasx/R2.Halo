@{
    ViewBag.Title = "Index";
    Layout = "~/Apps/Apps.DisasterBaseInfoStatistic/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Apps/Apps.DisasterBaseInfoStatistic/Content/ZHGL/Statistics.css")" rel="stylesheet" ) />
<script src="@Url.Content("~/Apps/Apps.DisasterBaseInfoStatistic/Scripts/Libs/highchartsVersion3.js")"></script>
